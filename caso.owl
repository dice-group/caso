@prefix aos: <http://example.org/aos#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# Ontology Declaration
aos:AccidentSeverityOntology a owl:Ontology ;
    rdfs:label "Accident Severity Ontology (AOS)" ;
    rdfs:comment "An ontology for modeling accident severity and related factors." .

# Classes
aos:Driver a owl:Class ;
    rdfs:label "Driver" ;
    rdfs:comment "Represents the driver involved in an accident." .

aos:Vehicle a owl:Class ;
    rdfs:label "Vehicle" ;
    rdfs:comment "Represents the vehicle involved in an accident." .

aos:Accident a owl:Class ;
    rdfs:label "Accident" ;
    rdfs:comment "Represents an accident event." .

aos:Road a owl:Class ;
    rdfs:label "Road" ;
    rdfs:comment "Represents the road conditions during an accident." .

aos:Environment a owl:Class ;
    rdfs:label "Environment" ;
    rdfs:comment "Represents the environmental conditions during an accident." .

aos:Casualty a owl:Class ;
    rdfs:label "Casualty" ;
    rdfs:comment "Represents the victims or casualties of an accident." .

# Properties (Attributes/Features)
## Driver Properties
aos:hasGender a owl:DatatypeProperty ;
    rdfs:domain aos:Driver ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Male" "Female" ) ] ;
    rdfs:label "Driver Gender" ;
    rdfs:comment "The gender of the driver." .

aos:hasAge a owl:DatatypeProperty ;
    rdfs:domain aos:Driver ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "18-30" "30-50" "Above 50" ) ] ;
    rdfs:label "Driver Age" ;
    rdfs:comment "The age group of the driver." .

aos:hasDrivingExperience a owl:DatatypeProperty ;
    rdfs:domain aos:Driver ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "1-2" "2-5" "5-10" "Above 10" ) ] ;
    rdfs:label "Driving Experience" ;
    rdfs:comment "The driving experience of the driver." .

aos:hasEducationLevel a owl:DatatypeProperty ;
    rdfs:domain aos:Driver ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Primary school" "Secondary school" "Above secondary school" "Others" ) ] ;
    rdfs:label "Education Level" ;
    rdfs:comment "The education level of the driver." .

aos:hasVehicleRelation a owl:DatatypeProperty ;
    rdfs:domain aos:Driver ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Employee" "Owner of the vehicle" ) ] ;
    rdfs:label "Vehicle Relation" ;
    rdfs:comment "The relationship of the driver to the vehicle." .

## Vehicle Properties
aos:hasType a owl:DatatypeProperty ;
    rdfs:domain aos:Vehicle ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Automobile" "Truck" "Pickup" "Public" "Taxi" "Motorcycle" "Others" ) ] ;
    rdfs:label "Vehicle Type" ;
    rdfs:comment "The type of vehicle involved in the accident." .

aos:hasServiceLife a owl:DatatypeProperty ;
    rdfs:domain aos:Vehicle ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "1" "1-2" "2-5" "5-10" "Above 10" ) ] ;
    rdfs:label "Service Life" ;
    rdfs:comment "The service life of the vehicle." .

aos:hasDefect a owl:DatatypeProperty ;
    rdfs:domain aos:Vehicle ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Brake defect" "Without defect" "Another mechanical defect" ) ] ;
    rdfs:label "Vehicle Defect" ;
    rdfs:comment "The defect status of the vehicle." .

## Accident Properties
aos:hasVehicleMovement a owl:DatatypeProperty ;
    rdfs:domain aos:Accident ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Moving backward" "Turning to the right" "Turning to the left" "Travelling straight" "Entering the path of separation" "Entering intersection" "Stopping" "Unknown" "Others" ) ] ;
    rdfs:label "Vehicle Movement" ;
    rdfs:comment "The movement of the vehicle during the accident." .

aos:hasCollisionType a owl:DatatypeProperty ;
    rdfs:domain aos:Accident ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Crushing an inanimate object" "Side-by-side collision" "Front and side collision" "Front and rear collision" "Hitting a parked vehicle" "Overturning" "Frontal collision" ) ] ;
    rdfs:label "Collision Type" ;
    rdfs:comment "The type of collision during the accident." .

aos:hasCause a owl:DatatypeProperty ;
    rdfs:domain aos:Accident ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Improperly starting up" "Driving over a certain speed" "Not driving at a distance" "Failure to give way to pedestrians" "Not giving priority to the vehicle" "Violating the sign of give way" "Improper passing another vehicle" "Driving left to the right" "Drunk driving" "Unknown" "Others" ) ] ;
    rdfs:label "Cause of Accident" ;
    rdfs:comment "The cause of the accident." .

aos:hasArea a owl:DatatypeProperty ;
    rdfs:domain aos:Accident ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Residential area" "Hospital area" "Entertainment area" "Market place area" "Office area" "School area" "Factory area" "In a rural area" "Church" "Others" ) ] ;
    rdfs:label "Accident Area" ;
    rdfs:comment "The area where the accident occurred." .

aos:hasSeverity a owl:DatatypeProperty ;
    rdfs:domain aos:Accident ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Fatal" "Serious injury" "Slight injury" "No injury" ) ] ;
    rdfs:label "Accident Severity" ;
    rdfs:comment "The severity of the accident." .

## Road Properties
aos:hasSurfaceType a owl:DatatypeProperty ;
    rdfs:domain aos:Road ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Good asphalt" "Perforated asphalt" "Gravel" "Paving" ) ] ;
    rdfs:label "Road Surface Type" ;
    rdfs:comment "The type of road surface." .

aos:hasLaneType a owl:DatatypeProperty ;
    rdfs:domain aos:Road ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Double carriage way" "Two ways" "One way" "Divided with solid line" "Divided with broken line" ) ] ;
    rdfs:label "Road Lane Type" ;
    rdfs:comment "The type of road lane." .

aos:hasAlignment a owl:DatatypeProperty ;
    rdfs:domain aos:Road ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Straight and bumpy road" "Straight and flat road" "Straight and slightly inclined" "Straight and highly inclined" "Uphill" "Slopy" "Others" ) ] ;
    rdfs:label "Road Alignment" ;
    rdfs:comment "The alignment of the road." .

aos:hasSurfaceCondition a owl:DatatypeProperty ;
    rdfs:domain aos:Road ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Dry" "Mud" "Wet" "Other" ) ] ;
    rdfs:label "Road Surface Condition" ;
    rdfs:comment "The condition of the road surface." .

aos:hasJunctionType a owl:DatatypeProperty ;
    rdfs:domain aos:Road ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "T shape" "Y shape" "No junction" "O shape" "Railroad crossing" "X shape" "Others" ) ] ;
    rdfs:label "Junction Type" ;
    rdfs:comment "The type of junction on the road." .

## Environment Properties
aos:hasWeatherCondition a owl:DatatypeProperty ;
    rdfs:domain aos:Environment ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Hot" "Cloud" "Good Air" "Heavy rain" "Cold" "Light rain" "Strong wind" "Others" ) ] ;
    rdfs:label "Weather Condition" ;
    rdfs:comment "The weather condition during the accident." .

aos:hasLightCondition a owl:DatatypeProperty ;
    rdfs:domain aos:Environment ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Sun rising" "In day light" "Sun setting" "Dark with no street light" "Dark with weak street light" "Dark with strong street light" "Others" ) ] ;
    rdfs:label "Light Condition" ;
    rdfs:comment "The light condition during the accident." .

## Casualty Properties
aos:hasClass a owl:DatatypeProperty ;
    rdfs:domain aos:Casualty ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Drivers" "Pedestrians" "Passenger" ) ] ;
    rdfs:label "Casualty Class" ;
    rdfs:comment "The class of the casualty (e.g., driver, pedestrian, passenger)." .

aos:hasWork a owl:DatatypeProperty ;
    rdfs:domain aos:Casualty ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Employed" "Unemployed" "Farmer" "Student" "Others" ) ] ;
    rdfs:label "Casualty Work" ;
    rdfs:comment "The occupation of the casualty." .

aos:hasFitness a owl:DatatypeProperty ;
    rdfs:domain aos:Casualty ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "Healthy" "Blind" "Disabled" "Drunk" "Unknown" "Others" ) ] ;
    rdfs:label "Casualty Fitness" ;
    rdfs:comment "The fitness or health condition of the casualty." .

aos:hasPedestrianMovement a owl:DatatypeProperty ;
    rdfs:domain aos:Casualty ;
    rdfs:range [ a owl:DataOneOf ;
                 owl:oneOf ( "While following a vehicle" "While walking with his left hand where there is no footpath" "While traveling on a road without a pedestrian crossing" "While crossing the pedestrian crossing line" "While walking on the sidewalk" "Sitting or lying down in the driveway" "Getting on or off a vehicle" "Others" ) ] ;
    rdfs:label "Pedestrian Movement" ;
    rdfs:comment "The movement of the pedestrian during the accident." .
